<!DOCTYPE html>
<html prefix="og: http://ogp.me/ns#">
<head>
    <title>@ViewBag.Title - Uniforms</title>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta property="fb:app_id" content="1833366153575850" />
    <meta property="og:title" content="Cabin Crew Uniforms" />
    <meta property="og:url" content="http://www.cabincrewuniforms.somee.com/" />
    <meta property="og:description" content="How well do you know your uniforms? Take this quiz to find out!" />
    <meta property="og:image" content="https://2.bp.blogspot.com/--YHFbqQhea4/Ur4AS6AOH5I/AAAAAAAAK9c/1m9QxvcjUds/s1600/airserbiacrew.png" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script type="text/javascript" src="~/Scripts/angular.min.js"></script>
    <script type="text/javascript" src="~/Scripts/angular-route.min.js"></script>
    <script type="text/javascript" src="~/Scripts/ui-bootstrap-tpls-2.1.3.min.js"></script>
    <link rel="stylesheet" type="text/css" href="~/Angular/Css/Style.css">
    <script type="text/javascript" src="~/Angular/JS/app.js"></script>
    <script type="text/javascript" src="~/Angular/JS/Home/homeCtrl.js"></script>
    <script type="text/javascript" src="~/Angular/JS/Quiz/quizCtrl.js"></script>
    <script type="text/javascript" src="~/Angular/JS/Results/resultsCtrl.js"></script>
    <script type="text/javascript" src="~/Angular/JS/ServerData/dataService.js"></script>
    <script type="text/javascript" src="~/Angular/JS/Quiz/quizStateService.js"></script>
    <script type="text/javascript" src="~/Angular/JS/Results/resultsService.js"></script>
</head>
<body ng-app="uniforms" class="ng-cloak">

    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Uniforms", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Companies", "Index", "Companies")</li>
                    <li>@Html.ActionLink("Add New Company", "Create", "Companies")</li>
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Uniforms</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
